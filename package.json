{
    "name": "jak-api",
    "version": "0.0.1",
    "description": "A API made by JAK",
    "author": "Jonak Adipta Kalita",
    "license": "MIT",
    "private": true,
    "main": "dist/api.js",
    "scripts": {
        "start": "node dist/api.js",
        "dev": "nodemon --watch",
        "prebuild": "rimraf dist",
        "build": "tsc",
        "test": "echo No Tests provided till now.",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "prettier": "prettier **/*.{ts,json,md}",
        "prettier:fix": "prettier --write **/*.{ts,json,md}",
        "format": "eslint --fix . && prettier --write **/*.{ts,json,md}",
        "check:version": "ncu",
        "prepare": "husky install && npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Jonak-Adipta-Kalita/JAK-API.git"
    },
    "keywords": [
        "api",
        "jak",
        "brawl-stars",
        "miraculous",
        "mughal-empire",
        "ben-10",
        "genshin-impact"
    ],
    "bugs": {
        "url": "https://github.com/Jonak-Adipta-Kalita/JAK-API/issues"
    },
    "homepage": "https://github.com/Jonak-Adipta-Kalita/JAK-API#readme",
    "dependencies": {
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "swagger-jsdoc": "^6.2.7",
        "swagger-ui-express": "^4.6.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.15",
        "@types/node": "17.0.4",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^5.14.0",
        "@typescript-eslint/parser": "^5.14.0",
        "autoprefixer": "^10.4.0",
        "eslint": "^8.11.0",
        "husky": "^7.0.4",
        "nodemon": "^2.0.20",
        "npm-check-updates": "^12.5.3",
        "postcss": "^8.4.5",
        "prettier": "^2.5.1",
        "typescript": "4.8.4"
    }
}
