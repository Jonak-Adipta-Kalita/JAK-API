{
	"name": "jak-api",
	"version": "0.0.3",
	"description": "A API for JAK",
	"author": "Jonak Adipta Kalita",
	"main": "server.js",
	"scripts": {
		"start": "nodemon server.js",
		"lint": "eslint **/*.js",
		"lint:fix": "prettier-eslint **/*.js --write"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Jonak-Adipta-Kalita/JAK-API.git"
	},
	"keywords": [
		"api",
		"details",
		"hobby",
		"favourite-food"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": ["npm run lint:fix", "git add"]
	},
	"dependencies": {
		"json-server": "^0.16.3"
	},
	"devDependencies": {
		"eslint": "^7.27.0",
		"husky": "^6.0.0",
		"lint-staged": "^11.0.0",
		"nodemon": "^2.0.7",
		"prettier-eslint": "^12.0.0",
		"prettier-eslint-cli": "^5.0.1"
	}
}
