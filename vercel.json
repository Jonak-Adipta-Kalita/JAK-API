{
    "version": 2,
    "builds": [
        {
            "src": "*.ts",
            "use": "@vercel/node-12",
            "config": {
                "experimentalTsConfig": true
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
