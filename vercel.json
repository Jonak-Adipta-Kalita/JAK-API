{
    "version": 2,
    "builds": [
        {
            "src": "./dist/api.js",
            "use": "@vercel/node",
            "config": {
                "command": "npm run build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
